<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config>
        <trust-anchors>
            <!-- 信任系统 CA -->
            <certificates src="system" />
            <!-- 信任用户自己安装的 CA -->
            <certificates src="user" />
        </trust-anchors>
    </base-config>
</network-security-config>
